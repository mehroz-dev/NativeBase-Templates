{
  "name": "@native-base/universal-app-template",
  "version": "1.0.0",
  "bugs": {
    "url": "https://github.com/GeekyAnts/nativebase-templates/issues"
  },
  "workspaces": [
    "apps/components",
    "apps/mobile",
    "apps/web"
  ],
  "description": "",
  "main": "index.js",
  "scripts": {
    "start:web": "yarn --cwd apps/web run dev",
    "start:app": "yarn --cwd apps/mobile run start",
    "start:app:ios": "yarn --cwd apps/mobile run ios",
    "start:app:android": "yarn --cwd apps/mobile run android",
    "start:components": "yarn --cwd apps/components run start"
  },
  "private": true,
  "keywords": [
    "Universal app",
    "NativeBase",
    "Next.js",
    "React Native"
  ],
  "author": "GeekyAnts",
  "license": "ISC",
  "devDependencies": {
    "eslint-config-react": "^1.1.7",
    "eslint-plugin-jest": "^24.4.0",
    "eslint-plugin-prettier": "^3.4.0",
    "eslint-plugin-react": "^7.24.0",
    "next-transpile-modules": "^8.0.0"
  },
  "resolutions": {
    "react": "^17.0.1"
  },
  "dependencies": {
    "@babel/runtime": "^7.15.3"
  }
}
